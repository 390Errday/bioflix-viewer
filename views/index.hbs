<div class="mdl-grid">
{{#movies}}
  <div class="mdl-cell mdl-cell--4-col">
    <div class="card-movie mdl-card mdl-shadow--2dp" onclick="location.href='/{{this.movie_name}}';">
      <div class="mdl-card__title mdl-card--expand"></div>
      <div class="mdl-card__actions">
        <span class="card-movie__filename">{{this.movie_name}}</span>
      </div>
    </div>
  </div>
{{/movies}}
</div>
